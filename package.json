{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "Nicholas W.Mazzei",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "mg:sequelize": "node_modules/.bin/sequelize db:migrate",
    "mg:sequelize:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "mg:sequelize:generate": "node_modules/.bin/sequelize migration:generate --name",
    "typeorm": "typeorm-ts-node-commonjs",
    "mg:typeorm": "typeorm-ts-node-commonjs migration:run --dataSource './ormconfig.ts'",
    "mg:typeorm:undo": "typeorm-ts-node-commonjs migration:revert --dataSource './ormconfig.ts'",
    "mg:typeorm:generate": "echo Type this command and put the migration name in MIGRATION_NAME \n echo npx typeorm-ts-node-commonjs migration:create ./src/@common/infra/db/typeorm/migrations/MIGRATION_NAME"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "mariadb": "^3.3.1",
    "mongoose": "^8.7.0",
    "mysql2": "^3.11.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.2.0",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.10",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.3.10",
    "@types/express": "^4.17.21",
    "@types/jest": "28.1.4",
    "@types/node": "^22.4.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "28.1.2",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@Common/(.*)$": "<rootDir>/../src/@common/$1"
    }
  }
}
